<Window x:Class="ITMO.CSharp.WPF.Lab1_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ITMO.CSharp.WPF.Lab1_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="BtnSetName" Content="Set Name" HorizontalAlignment="Left" Height="80" Margin="81,83,0,0" VerticalAlignment="Top" Width="152" Click="Button_Click"/>
        <Button x:Name="BtnRetName" Content="Ret Name" HorizontalAlignment="Left" Height="80" Margin="81,216,0,0" VerticalAlignment="Top" Width="152" Click="Button_Click_1"/>
        <TextBox x:Name="NameInput" HorizontalAlignment="Left" Height="80" Margin="335,83,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="380" TextChanged="NameInput_TextChanged"/>
        <Label x:Name="NameOutput" Content="" HorizontalAlignment="Left" Height="80" Margin="335,216,0,0" VerticalAlignment="Top" Width="380"/>
        <Button Canvas.Left="10" Canvas.Top="10" Content="Click Me" Height="23" Name="button3" Width="75" Click="Button_Click_3"/>
        <TextBox Width="170" Canvas.Left="90" Canvas.Top="12" Name="myTextBox" />
        
    </Canvas>
</Window>
